{
  "name": "fastify-auth-skeleton",
  "version": "0.3",
  "description": "A skeleton project with typescript typeorm & jwt authentication",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "npx rimraf build",
    "build": "tsc",
    "start": "fastify start -l info build/app.js",
    "dev": "npm run clean && npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P build/app.js\""
  },
  "keywords": [],
  "author": "SEK-Consulting",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.4.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/rate-limit": "^7.4.0",
    "@fastify/sensible": "^5.1.1",
    "bcrypt": "^5.0.1",
    "fastify": "^4.5.3",
    "fastify-cli": "^5.4.1",
    "fastify-plugin": "^4.2.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.7.17",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}

{
  "name": "fastify-api-skeleton",
  "version": "1.0.0",
  "description": "a typescript skeleton project with prisma & jwt authentication",
  "main": "app.ts",
  "scripts": {
    "gen": "npx prisma format && npx prisma generate",
    "clean": "npx rimraf build",
    "build": "tsc",
    "start": "fastify start -l info build/app.js",
    "dev": "npm run clean && npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P build/app.js\""
  },
  "keywords": [],
  "author": "SEK-Consulting",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/sensible": "^5.2.0",
    "@prisma/client": "^4.10.1",
    "bcrypt": "^5.1.0",
    "fastify": "^4.13.0",
    "fastify-plugin": "^4.5.0",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.10.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
